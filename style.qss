QHeaderView::section{
    background-color: #FFFBE7;
    color: black;
    font-size: 16px;
    font-weight: bold;
    padding: 5px;
    border: 1px solid black;
}

QRadioButton:disabled{
        color: rgba(0, 0, 0, 0.3); /* Color más claro */
}


/* Checkbox deshabilitado sin estar marcado */
QCheckBox:disabled {
    color: rgba(0, 0, 0, 0.3); /* Color más claro */
}

/* Checkbox deshabilitado y marcado */
QCheckBox:disabled:checked {
    color: black; /* Color normal */
}

/* Checkmark azul para checkbox marcado y deshabilitado */
QCheckBox::indicator:disabled:checked {
    image: url(data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 24 24'><path fill='%23007AFF' d='M20.66 5.34a1 1 0 010 1.41l-10 10a1 1 0 01-1.41 0l-4-4a1 1 0 011.41-1.41L10 14.59l9.25-9.25a1 1 0 011.41 0z'/></svg>);
}







