
#btnBuscarCli{
background-color: #13b2a6;
    font-weight: bold;
    color: white;
    border-radius: 5px;
}

#btnBuscarCli:hover {
    background-color: #11a194;
}

#btnBuscarCli:pressed {
    background-color: #0f897e;
    padding-left: 2px; /* Para dar la sensación de que el botón se hunde */
    padding-top: 2px;
}

#btnBuscarCli:checked {
    background-color: #0f897e;
    padding-left: 2px; /* Para dar la sensación de que el botón se hunde */
    padding-top: 2px;
}




